if DX_COND_doc
dbo.tag: doxygen.conf
	PROJECT_NAME="$(PACKAGE)" \
	PROJECT_NUMBER="$(PACKAGE_VERSION)" \
	top_srcdir="$(top_srcdir)" \
	doxygen doxygen.conf
	mkdir -p man/man3 man/man7
	srcdir="$(top_srcdir)/src" ./gen-man-pages.pl xml/*.xml

all-local: dbo.tag

install-data-local:
	$(MKDIR_P) "$(docdir)"/html/
	$(INSTALL_DATA) html/*.{html,css,js,png} "$(docdir)"/html/
	$(MKDIR_P) "$(docdir)"/html/search/
	$(INSTALL_DATA) html/search/*.{html,css,js,png} "$(docdir)"/html/search/

$(MANPAGES3) $(MANPAGES7): dbo.tag

man3_MANS = $(MANPAGES3)
man7_MANS = $(MANPAGES7)
endif
